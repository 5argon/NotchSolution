{
  "metadata": [
    {
      "src": [
        {
          "src": "../../../",
          "files": [
            "NotchSolution.sln"
          ]
        }
      ],
      "dest": "../.documentation/api",
      "disableGitFeatures": true,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "src" : "..",
        "files": [
          "CHANGELOG.md"
        ]
      },
      {
        "files": [
          "api/*.yml"
        ],
        "dest" : "api"
      },
      {
        "src" : "../.documentation",
        "files": [
          "*.md",
          "*.yml",
          "**/*.md",
          "**/*.yml"
        ]
      }
    ],
    "resource": [
      {
        "src" : "../.documentation",
        "files": [
          "**/images/*"
        ]
      },
      {
        "src" : "../",
        "files": [
          "Icons/*"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true 
  }
}